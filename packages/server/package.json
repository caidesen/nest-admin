{
  "name": "@nest-admin/server",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "typings": "src/index.d.ts",
  "main": "src/index.d.ts",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "nestia:sdk": "nestia sdk",
    "db:refresh": "ts-node -r tsconfig-paths/register script/database/refresh.ts",
    "prepare": "ts-patch install && typia patch"
  },
  "dependencies": {
    "@fastify/cookie": "^9.3.1",
    "@fastify/helmet": "^11.1.1",
    "@mikro-orm/better-sqlite": "^6.2.1",
    "@mikro-orm/core": "^6.2.1",
    "@mikro-orm/nestjs": "^5.2.3",
    "@nestia/core": "^2.0.1",
    "@nestjs/cache-manager": "^2.2.2",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.0",
    "@nestjs/platform-fastify": "^10.3.8",
    "bcrypt": "^5.1.1",
    "cache-manager": "^5.5.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.0.3",
    "fastify": "^4.26.2",
    "lodash": "^4.17.21",
    "mikro-orm-plus": "^1.0.0",
    "nestjs-cacheable": "^1.0.0",
    "nestjs-pino": "^4.0.0",
    "pino-http": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "typia": "^5.3.11"
  },
  "devDependencies": {
    "@mikro-orm/cli": "^6.2.1",
    "@mikro-orm/seeder": "^6.2.1",
    "@nestia/e2e": "^0.3.6",
    "@nestia/fetcher": "^2.4.5",
    "@nestia/sdk": "^2.0.1",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.14.194",
    "@types/node": "^20.3.1",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.0",
    "fast-glob": "^3.3.2",
    "jest": "^29.5.0",
    "nestia": "^5.1.2",
    "pino-pretty": "^10.2.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  },
  "mikro-orm": {
    "useTsNode": true,
    "configPaths": [
      "./src/mikro-orm.config.ts",
      "./dist/mikro-orm.config.js"
    ]
  }
}